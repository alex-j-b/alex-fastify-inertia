{
  "name": "fastify-inertia-vue",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "vite build --outDir build/client",
    "build:ssr": "vite build --outDir build/ssr --ssr src/ssr.js"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@inertiajs/vue3": "^2.1.2",
    "fastify": "^5.5.0",
    "fastify-inertiajs": "^1.0.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "cross-env": "^10.0.0",
    "vite": "^7.1.2"
  }
}
